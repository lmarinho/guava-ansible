---
title: Work by Guava
---

<% content_for :additional_js do %>
  <%= javascript_include_tag :'section-highlight' %>
  <%= javascript_include_tag :'dynamic-menu' %>
  <%= javascript_include_tag :'logo-transition' %>
<% end %>

<% content_for :language_link do %>
  <%= partial 'language-link', locals: { type: 'short', color: 'red' }%>
<% end %>

<header id="cover">
  <div class="cover of-work">
    <span class="logo of-guava at-cover">
      <%= inline_svg 'guava' %>
    </span>
    <div class="grid is-container of-hero">
      <div class="grid is-row">
        <div class="grid is-content">
          <h1><%= t(:'work.title') %></h1>
          <p class="is-lead also-serif hasnt-widow"><%= t(:'work.subtitle') %></p>
          <p><%= t(:'work.intro') %></p>
        </div>
      </div>
    </div>
  </div>
  <span class="icon of-arrow-down at-work / js-scroll-indicator"
    data-scroll-indicator-target=".page-work.is-project.also-regular" data-scroll-indicator-offset="100">
    <%= inline_svg 'icon-arrow-big', class: 'icon of-big-arrow' %>
  </span>
</header>

<hr>

