              <figure class="is-image of-trueandco">
                <%= image_tag 'cases/true&co.jpg', alt: t(:'pictures.trueandco') %>
              </figure>
              <figure>
                <%= image_tag t(:'files.team-aug.small'), class: 'is-stamp of-team', alt: t(:'graphics.stamps.team-aug') %>
                <figcaption><%= t(:'work.stamps.team-aug') %></figcaption>
              </figure>
            </header>
            <section class="is-content">
              <figure class="is-client">
                <%= inline_svg 'client-trueandco', class: 'logo of-client is-trueandco' %>
