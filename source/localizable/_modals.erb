<div id="chico" class="modal is-backdrop" tabindex="-1" role="dialog" aria-labelledby="About Chico" aria-hidden="true">
  <div class="modal is-wrapper" role="document">
    <figure role="presentation">
      <%= image_tag 'illustration-team-chico-small@2x.png', class: '', alt: t(:'illustrations.team.chico') %>
    </figure>
    <h3><%= t(:'about-us.team.chico.title') %></h3>
    <p><%= t(:'about-us.team.chico.role') %></p>
    <p><%= t(:'about-us.team.chico.text') %></p>
    <p><%= t(:'about-us.team.chico.background') %></p>
    <% link_to 'https://www.linkedin.com/in/chico-carvalho-a8352326', class: '', title: t(:'social.linkedin'), target: 'blank' do %>
        <%= t('social.linkedin') %>
      <% end %>
    <button data-dismiss="modal" title="<%= t('button.close') %>"></button>
  </div>
</div>

